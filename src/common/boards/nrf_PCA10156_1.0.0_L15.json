                "state": true
            },
            {
                "pin": 23,
                "state": false
            },
            {
                "pin": 45,
                "state": true
            },
            {
                "pin": 6,
                "state": true
            },
            {
                "pin": 47,
                "state": true
            }
        ],
        "pmicPorts": [
            {
                "port": 1,
                "voltage": 1800
            }
        ]
    }
